{
  "name": "genjijs",
  "npmName": "genjijs",
  "tnpmName": "@tencent/genji",
  "scripts": {
    "test": "jest --config jest.config.js --coverage",
    "testinci": "jest --config jest.config.js --coverage --coverageReporters=text-lcov | coveralls",
    "dev": "webpack-dev-server --open --config webpack.config.dev.js",
    "build": "webpack",
    "fix": "npm run fix:eslint && npm run fix:prettier",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lint:eslint": "eslint \"{app,example,mock,test}/**/*.{js,jsx,ts,tsx}\"",
    "fix:eslint": "eslint \"{app,example,mock,test}/**/*.{js,jsx,ts,tsx}\" --fix",
    "lint:prettier": "prettier --check \"{app,example,mock,test}/**/*.{js,jsx,ts,tsx}\"",
    "fix:prettier": "prettier --write \"{app,example,mock,test}/**/*.{js,jsx,ts,tsx}\""
  },
  "version": "0.2.0",
  "description": "Explore a better way to manage data state",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kelekexiao123/genjijs.git"
  },
  "author": "kelekexiao123,huangruichang",
  "license": "MIT",
  "keywords": [
    "react",
    "redux",
    "dva",
    "redux-thunk"
  ],
  "bugs": {
    "url": "https://github.com/kelekexiao123/genjijs/issues"
  },
  "homepage": "https://github.com/kelekexiao123/genjijs#readme",
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "@testing-library/react": "^9.4.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "coveralls": "^3.0.9",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-react": "^7.17.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.0.1",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "webpack": "^4.41.2",
    "webpack-api-mocker": "^1.7.6",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.9.0"
  },
  "dependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-export-default-from": "^7.5.2",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/polyfill": "^7.6.0",
    "@babel/preset-react": "^7.6.3",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-redux": "^7.1.1",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run test"
    }
  }
}
